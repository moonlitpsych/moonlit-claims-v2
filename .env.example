# IntakeQ API
INTAKEQ_API_KEY=your_intakeq_api_key_here

# Office Ally - SFTP
OFFICE_ALLY_SFTP_HOST=your_sftp_host
OFFICE_ALLY_SFTP_USER=your_sftp_username
OFFICE_ALLY_SFTP_PASSWORD=your_sftp_password
OFFICE_ALLY_SFTP_PORT=22

# Office Ally - REALTIME API
OFFICE_ALLY_REALTIME_ENDPOINT=https://wsd.officeally.com/TransactionService/rtx.svc
OFFICE_ALLY_REALTIME_API_KEY=your_realtime_api_key
OFFICE_ALLY_SENDER_ID=your_sender_id
OFFICE_ALLY_PROVIDER_NPI=your_provider_npi

# Google Gemini API (HIPAA-configured)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-pro

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Application
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000
NODE_ENV=development

# Logging
LOG_LEVEL=info
ENABLE_AUDIT_LOGGING=true
